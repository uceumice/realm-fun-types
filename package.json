  },
  "homepage": "https://github.com/realm-fun-types#readme",
  "devDependencies": {
    "prettier": "^2.7.1",
    "tslint-config-prettier": "^1.18.0",
    "eslint": "^8.26.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "bson": "^4.7.0",
    "mongodb": "^4.11.0"
